datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Neww {
id             String          @id @default(auto()) @map("_id") @db.ObjectId
  title    String
  tobic  String
  place  String
  createdAt DateTime @default(now()) //يضيف لك التاريخ تلقائي
  
}






